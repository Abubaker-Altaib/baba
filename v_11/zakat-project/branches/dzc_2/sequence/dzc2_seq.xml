<?xml version="1.0" encoding="utf-8"?>
<openerp>
 <data >


  <record forcecreate="1" id="dzc2_seq" model="ir.sequence">
   <field name="name">project request sequence</field>
   <field name="code">dzc2.project.request.sequence</field>
   <field name="prefix">%(year)s/%(month)s/0000</field>
   <field name="active">True</field>
   <field eval="1" name="number_increment"/>
  </record>
 
  <record forcecreate="1" id="dzc2_planning_seq" model="ir.sequence">
   <field name="name">project planning sequence</field>
   <field name="code">dzc2.project.planning.sequence</field>
   <field name="prefix">%(year)s/%(month)s/0000</field>
   <field name="active">True</field>
   <field eval="1" name="number_increment"/>
  </record>
  

  <record forcecreate="1" id="dzc2_states_support_seq" model="ir.sequence">
   <field name="name">prepairing states support sequence</field>
   <field name="code">dzc2.prepairing.states.sequence</field>
   <field name="prefix">SP/%(year)s/0000</field>
   <field name="active">True</field>
   <field eval="1" name="number_increment"/>
  </record>

 </data>
</openerp>