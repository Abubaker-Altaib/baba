<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!--    ________________  iban alsabil Seq   __________________         -->
        <!--    Zakat/dzc_8/dzc_8 model/Reference Number         -->

        <record id="dac_8_order" model="ir.sequence">
            <field name="name">iban alsabil seq</field>
            <field name="code">zakat.dzc8</field>
            <field name="prefix">%(year)s/%(month)s/0000</field>
	        <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>


        <!--    ________________  iban alsabil Payment Seq   __________________         -->
        <!--    Zakat/dzc_8/IbnAlsabilPayment model/Reference Number         -->

        <record id="dac_8_payment" model="ir.sequence">
            <field name="name">iban alsabil payment seq</field>
            <field name="code">zakat.dzc8.payment</field>
            <field name="prefix">%(year)s/%(month)s/0000</field>
	        <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</openerp>
