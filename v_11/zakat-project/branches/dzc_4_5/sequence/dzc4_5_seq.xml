<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        
        <record id="dawa_plan_seq" model="ir.sequence">
            <field name="name">Dawa Plan seq</field>
            <field name="code">dawa.activities.plan.sequence</field>
            <field name="prefix">%(year)s/%(month)s/0000</field>
	        <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>

        <!--    ________________  Dawa Activity Request   __________________         -->
        <!--    Zakat/dzc_8/dzc_8 model/Reference Number         -->

        <record id="dawa_activity_request_order" model="ir.sequence">
            <field name="name">Daea Activity Request Sequance</field>
            <field name="code">dawa.activity.request.sequance</field>
            <field name="prefix">%(year)s/%(month)s/0000</field>
	        <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</openerp>
