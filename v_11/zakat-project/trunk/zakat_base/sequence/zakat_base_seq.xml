<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!--    ________________  Zakat Partner Seq   __________________         -->
        <!--    Zakat/zakat_base/ZakatResPartner model/Reference Number         -->

        <record id="zakat_partner_seq" model="ir.sequence">
            <field name="name">zakat partner seq</field>
            <field name="code">res.partner</field>
            <field name="prefix">%(year)s/</field>
	        <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>
        
        <!-- Channels Sequence -->
        <record id="zakat_channels_seq" model="ir.sequence">
            <field name="name">zakat Channels seq</field>
            <field name="code">zakat.channels</field>
            <field name="prefix">0000</field>
	        <field name="use_date_range">True</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</openerp>
