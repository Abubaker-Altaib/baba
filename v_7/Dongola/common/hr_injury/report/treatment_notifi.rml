					<para style="col_Right">
						[[ t.voucher_id and t.voucher_id.number or removeParentNode('tr') ]]
					</para>
				</td>
			</tr>
		</blockTable>
		<blockTable colWidths="0,0,550,0" style="Table_with_header">
			[[ has_treatment(p) and removeParentNode('blockTable') ]]
			<tr>
				<td>
					<para style="col_center"></para>
				</td>
				<td>
					<para style="col_center"></para>
				</td>
				<td>
					<para style="col_center"> لا يوجد فواتير علاجية مرحلة</para>
				</td>
				<td>
					<para style="col_center"></para>
				</td>
