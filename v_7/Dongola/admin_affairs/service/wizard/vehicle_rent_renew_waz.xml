<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

<!-- Vehicle Rent Contract Renew Wizard View -->
	<record id="vehicle_rent_renew_form" model="ir.ui.view">
		<field name="name">vehicle_rent_renew</field>
		<field name="model">vehicle.rent.renew</field>
		<field name="arch" type="xml">
			<form string="Vehicle rent renew wizard" version="7.0">
				<group>
					<separator string="Vehicle rent renew" colspan="4"/>
					<field name="date_from" />
					<field name="date_to" />
				</group>
				<footer>
					<button name="renew_contract" string="Renew" type="object" class="oe_highlight"/>
					or
					<button special="cancel" string="Cancel" class="oe_link"/>
				</footer>
			</form>
		</field>
	</record>

    </data>
</openerp>
