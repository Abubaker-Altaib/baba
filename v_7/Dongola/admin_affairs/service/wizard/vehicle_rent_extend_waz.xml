<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
<!-- Vehicle Rent Contract extend Wizard View -->
	<record id="vehicle_rent_extend_form" model="ir.ui.view">
		<field name="name">vehicle_rent_extend</field>
		<field name="model">vehicle.rent.extend</field>
		<field name="arch" type="xml">
			<form string="Vehicle rent extend wizard" version="7.0">
				<group>
					<separator string="Vehicle rent extend" colspan="4"/>
					<field name="extend_date" />
					<newline/>
				</group>
				<footer>
					<button name="extend_contract" string="Extend" type="object" class="oe_highlight"/>
					or
					<button special="cancel" string="Cancel" class="oe_link"/>
				</footer>
			</form>
		</field>
	</record>

    </data>
</openerp>
