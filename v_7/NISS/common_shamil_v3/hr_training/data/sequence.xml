<?xml version="1.0"?>
<openerp>
	<data noupdate="1">
		<!-- Suggested Courses -->
		<record id="seq_type_suggested_course" model="ir.sequence.type">
			<field name="name">Suggested Courses</field>
			<field name="code">hr.suggested.course</field>
		</record>
		<record id="seq_suggested_course" model="ir.sequence">
			<field name="name">Suggested Courses</field>
			<field name="code">hr.suggested.course</field>
			<field eval="3" name="padding" />
			<field name="prefix">%(year)s/</field>
		</record>
		
		<!-- Approved Courses -->
		<record id="seq_type_approved_course" model="ir.sequence.type">
			<field name="name">Approved Courses</field>
			<field name="code">hr.approved.course</field>
		</record>
		<record id="seq_approved_course" model="ir.sequence">
			<field name="name">Approved Courses</field>
			<field name="code">hr.approved.course</field>
			<field eval="3" name="padding" />
			<field name="prefix">%(year)s/</field>
		</record>
	</data>
</openerp>
