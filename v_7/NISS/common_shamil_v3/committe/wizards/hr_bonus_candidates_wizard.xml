<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="hr_bonus_v" model="ir.ui.view">
            <field name="name">Bonus Candidates</field>
            <field name="model">hr.bonus.candidates</field>
            <field name="inherit_id" ref="hr_payroll_custom_niss.hr_bonus_v" />
            <field name="arch" type="xml">
                <form string="Bonus Candidates" version="7.0">
                    <xpath expr="//field[@name='academic']" position="after">
                        <group>
                            <field name="is_committe" string="Have Committe"/>
                        </group>
                    </xpath>
                </form>
            </field>
        </record>
    </data>
</openerp>