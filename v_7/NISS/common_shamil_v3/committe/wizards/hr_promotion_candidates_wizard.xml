<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="hr_promotions_candidates" model="ir.ui.view">
            <field name="name">Promotions Candidates</field>
            <field name="model">hr.promotions.candidates</field>
            <field name="inherit_id" ref="hr_payroll_custom_niss.hr_promotions_candidates" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='rebuke']" position="after">
                    <field name="is_committe" string="Have Committe"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>