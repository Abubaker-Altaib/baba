<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for contract --> 
        <record id="seq_type_purchase_contract" model="ir.sequence.type">
            <field name="name">Purchase Contract</field>
            <field name="code">purchase.contract</field>
        </record>
        <record id="seq_purchase_contract" model="ir.sequence">
            <field name="name">Purchase Contract</field>
            <field name="code">purchase.contract</field>
            <field name="prefix">%(year)s/C-</field>
            <field name="padding">5</field>
        </record>

        <!-- Contract Shipment Sequences --> 
        <record id="seq_type_contract_shipment" model="ir.sequence.type">
            <field name="name">Contract Shipment</field>
            <field name="code">contract.shipment</field>
        </record>
        <record id="seq_contract_shipment" model="ir.sequence">
            <field name="name">Contract Shipment</field>
            <field name="code">contract.shipment</field>
            <field name="prefix">%(year)s/CS-</field>
            <field name="padding">5</field>
        </record>
        
        <!-- Contract fees Sequences --> 
        <record id="seq_type_contract_fees" model="ir.sequence.type">
            <field name="name">Contract Fees</field>
            <field name="code">contract.fees</field>
        </record>
        <record id="seq_contract_fees" model="ir.sequence">
            <field name="name">Contract Fees</field>
            <field name="code">contract.fees</field>
            <field name="prefix">%(year)s/CF-</field>
            <field name="padding">5</field>
        </record>
    </data>
</openerp>
