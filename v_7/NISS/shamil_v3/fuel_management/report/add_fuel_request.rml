                        <tr>
                            <td>
                                <para style="small_col_center"> اﻹجمالي </para>
                            </td>
                            <td>
                                <para style="small_col_center"> العدد </para>
                            </td>
                            <td>
                                <para style="small_col_center"> الكمية </para>
                            </td>
                            <td>
                                <para style="small_col_center"> نوع المركبة </para>
                            </td>
                        </tr>
                        <tr>[[ repeatIn( p.move_line_id, 'line' ) ]]
                            <td>
                                <para style="small_col_Right">[[ formatLang(line.total) ]]</para>
                            </td>
                            <td>
                                <para style="small_col_Right">[[ formatLang(line.number) ]]</para>
                            </td>
                            <td>
                                <para style="small_col_Right">[[ formatLang(line.amount) ]]</para>
                            </td>
                            <td>
                                <para style="small_col_Right">[[ line.vehicle_type.name ]]</para>
                            </td>
                        </tr>
                    </blockTable>
                </section>


                <section> [[ p.move_line_id[0].fuel_pattarn == 'different' or removeParentNode('section') ]]
