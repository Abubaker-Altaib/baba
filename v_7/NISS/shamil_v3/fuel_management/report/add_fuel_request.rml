                    <blockTable colWidths="260,260" style="Table_with_frame3">
                        <tr>
                            <td>
                                <para style="small_col_center"> الكمية </para>
                            </td>
                            <td>
                                <para style="small_col_center"> المركبة </para>
                            </td>
                        </tr>
                        <tr>[[ repeatIn( get_vhc(p.move_line_id), 'line' ) ]]
                            <td>
                                <para style="small_col_Right">[[ formatLang(line.amount) ]]</para>
                            </td>
                            <td>
                                <para style="small_col_Right">[[ line.vehicle_id.name ]]</para>
                            </td>
                        </tr>
                    </blockTable>
                </section>

            </section>

        </section>

        <section> [[ p.add_type == 'additional' or removeParentNode('section') ]]
        <para style="P12_R">بيانات المركبة :-</para>
        
        
        <blockTable colWidths="270,270" style="Table_title">
            <tr>
                <td>
                    <para style="P12_L">رقم الشاسي : [[ p.vehicle_id.vin_sn ]]</para>
                </td>
                <td>
                    <para style="P12_R">المركبة : [[ p.vehicle_id.name ]]</para>
                </td>
                
            </tr>
            <tr>
                <td>
                    <para style="P12_L">[[p.department_id and ('اﻹدارة :'+p.department_id.name) or '']]</para>
                </td>
                <td>
                    <para style="P12_R">اللوحة : [[ p.vehicle_id.code_plate ]]</para>
                </td>
            </tr>
            <tr>
                <td>
                    <para style="P12_L">[[p.vehicle_id.employee_id and ('اﻹدارة :'+p.vehicle_id.employee_id.name) or '']]</para>
                </td>
