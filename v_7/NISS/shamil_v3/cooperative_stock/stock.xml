<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>



<!-- inherit Field from Product Category -->
<record model="ir.ui.view" id="view_product_category_stock_cooperative">
		<field name="name">product.category.stock_cooperative</field>
		<field name="model">product.category</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="account.view_category_property_form"/>
		<field name="arch" type="xml">
			<field name="name" position="after">
		              <group colspan="4">
                                     <field name="co_operative" colspan="2" /> 
		              </group>
		        </field>
                                

		</field>
	</record>



<!-- inherit Field from Stock Location -->
<record model="ir.ui.view" id="view_stock_location_stock_cooperative">
		<field name="name">stock.location.stock_cooperative</field>
		<field name="model">stock.location</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="stock.view_location_form"/>
		<field name="arch" type="xml">
			<field name="name" position="after">
		              <group colspan="4">
                                     <field name="co_operative" colspan="2" /> 
		              </group>
		        </field>
                                

		</field>
	</record>

</data>
</openerp>
