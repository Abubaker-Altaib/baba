<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>



<!-- Add Field To Account Account Form  -->
<record model="ir.ui.view" id="view_account_account_cooperative">
		<field name="name">account.account.cooperative</field>
		<field name="model">account.account</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="account.view_account_form"/>
		<field name="arch" type="xml">
			<field name="name" position="after">
		              <group colspan="4">
                                     <field name="co_operative" colspan="2" /> 
		              </group>
		        </field>
                                

		</field>
	</record>



<!-- Add Field To Account Journal Form  -->
<record model="ir.ui.view" id="view_account_journal_cooperative">
		<field name="name">account.journal.cooperative</field>
		<field name="model">account.journal</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="account.view_account_journal_form"/>
		<field name="arch" type="xml">
			<field name="name" position="after">
		              <group colspan="4">
                                     <field name="co_operative" colspan="2" /> 
		              </group>
		        </field>
                                

		</field>
	</record>

</data>
</openerp>
