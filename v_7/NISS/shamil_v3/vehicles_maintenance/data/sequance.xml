		</record>

		<!-- Maintenance request -->
		<record id="seq_type_maintenance_request" model="ir.sequence.type">
			<field name="name">Maintenance request</field>
			<field name="code">maintenance.request</field>
		</record>
		<record id="seq_maintenance_request" model="ir.sequence">
			<field name="name">Maintenance request</field>
			<field name="code">maintenance.request</field>
			<field eval="7" name="padding" />
			<field name="prefix">RE/%(year)s/%(month)s/</field>
		</record>
	</data>
</openerp>