                    </group>
                    <group expand="0" string="Group By..." colspan="4" col="10">
                        <filter icon="terp-dialog-close" string="Date" context="{'group_by':'date'}"/>
                        <filter icon="terp-dialog-close" string="Custody Type" context="{'group_by':'custody_type'}"/>
                        <filter icon="terp-dialog-close" string="State" context="{'group_by':'state'}"/>
                    </group>
                </search>
            </field>
        </record>





