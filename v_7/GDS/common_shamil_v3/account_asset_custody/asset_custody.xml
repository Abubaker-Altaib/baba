<?xml version="1.0"?>
<openerp>
    <data>
        <!--menuitem id="menu_asset_custody" name="Asset Custody" sequence="10"/-->
        <menuitem id="menu_asset_custody_parent" name="Asset Custody" parent="stock.menu_stock_root" sequence="10"/>
        <!--    Reports Menu    -->
        <menuitem id="menu_asset_custody_report" name="Report" sequence="10"/>
        <menuitem id="menu_asset_custody_report_parent" name="Report" parent="stock.menu_stock_root" sequence="10"/>
        <!--    configuration Menu    -->
        <menuitem id="menu_asset_custody_configuration" name="Configuration" sequence="10"/>
        <menuitem id="menu_asset_custody_configuration_parent" name="Configuration" parent="stock.menu_stock_root" sequence="15"/>
        <!--    Reports     -->
        <report auto="False" id="asset_barcode_print" model="account.asset.asset" name="asset.print.barcode" rml="asset_custody/report/asset_barcode_print.rml" string="Asset Barcode" />
        <!--    Asset custody    -->
        <record model="ir.ui.view" id="view_account_asset_asset_form2">
            <field name="name">account.asset.asset.form2</field>
            <field name="model">account.asset.asset</field>
            <field name="arch" type="xml">
                <form string="Asset" version="7.0" create="false">
                    <header>
                        <button name="validate" states="draft" string="Confirm Asset" type="object" />
                        <field name="state" widget="statusbar" statusbar_visible="draft,confirmed,closed"/>
                    </header>
                    <sheet>
                        <div class="oe_button_box oe_right">
                            <button name="open_entries" string="Entries" type="object" class="oe_inline" invisible="1"/>
                        </div>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only"/>
                            <h1>
                                <field name="name" class="oe_inline"/>
                            </h1>
                        </div>
                        <group invisible="1">
                            <group>
                                <field name="category_id" on_change="onchange_category_id(category_id)"/>
                                <field name="code"/>
                                <field name="parent_id"/>
                                <field name="state" />
                            </group>
                            <group >
                                <field name="purchase_date" invisible="1"/>
                                <field name="currency_id" groups="base.group_multi_currency" invisible="1"/>
                                <!--field name="company_id" widget="selection" groups="base.group_multi_company" on_change="onchange_company_id(company_id)"/-->
                            </group>
                        </group>
                        <!-- <group><field name="company_id" widget="selection" groups="base.group_multi_company" on_change="onchange_company_id(company_id)"/></group> -->
                        <group colspan="4" col="4" string="Vechile"  attrs="{'invisible':[('vechile','==',False)]}">
 
 
                            <field name="chassis"/>
                            <field name="number"/>
                            <field name="machine_no"/>
 
                            <field name="fuel"/>
                            <field name="model"/>
                            
                        </group>
 <group colspan="4" col="4" string="General Info">
                            <field name="product_id"/>
  
                             <field name="vechile"/>
                             
                            <field name="main_type"/>
                             
                            <field name="executing_agency"/>
                            <field name="department_id"/>
                            <field name="office_id" />
                            <field name="custody_type"/>
                            <field name="time_scale"/>
                            <field name="employee_id"/>
                            <field name="request_date"/>
                            <field name="return_date"/>
                            <field name="state_rm" />
                            <field name="company_id" widget="selection" groups="base.group_multi_company" on_change="onchange_company_id(company_id)"/>
                        </group>
                        <notebook>
                            <page string="custody Line" invisible="1">
                                <field name="custody_lines" readonly="True">
                                    <tree string="custody " editable="buttom">
                                        <field name="department_to" />
                                        <field name="office_id" />
                                        <field name="employee_to" />
                                        <field name="referance" />
                                        <field name="date" />
                                        <field name="type" />
                                        <!--field name="request_date"  /-->
                                    </tree>
                                </field>
                            </page>
                        </notebook >
                        <notebook >
                            <page string="History" invisible='1'>
                                <field name="account_move_line_ids" readonly="1"/>
                                <field name="history_ids" readonly="1"/>
                            </page>
                            <page string="Notes">
                                <field name="note" placeholder="Add an internal note here..."/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="view_account_asset_asset_tree">
            <field name="name">account.asset.asset.tree</field>
            <field name="model">account.asset.asset</field>
            <field name="field_parent">child_ids</field>
            <field name="arch" type="xml">
                <tree string="Assets" colors="blue:(state == 'draft');grey:(state == 'close')" create="false">
                    <field name="product_id"/>
 
                    <field name="main_type"/>
                    <field name="executing_agency"/>
                    <field name="department_id"/>
                    <field name="office_id" />
                    <field name="custody_type"/>
                    <field name="time_scale"/>
                    <field name="employee_id"/>
                    <field name="request_date"/>
                    <field name="return_date"/>
                    <field name="state_rm" />
                    <field name="state" />
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="view_account_asset_asset_hierarchy_tree">
            <field name="name">account.asset.asset.hierarchy</field>
            <field name="model">account.asset.asset</field>
            <field name="field_parent">child_ids</field>
            <field name="arch" type="xml">
                <tree string="Assets">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="category_id"/>
                    <field name="purchase_date"/>
                    <field name="purchase_value"/>
                    <field name="value_residual"/>
                    <field name="currency_id" groups="base.group_multi_currency"/>
                    <field name="company_id" groups="base.group_multi_company"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record id="view_account_asset_search" model="ir.ui.view">
            <field name="name">account.asset.asset.search</field>
            <field name="model">account.asset.asset</field>
            <field name="arch" type="xml">
                <search string="Account Asset">
                    <field name="name" string="Asset"/>
                    <field name="purchase_date"/>
                    <filter icon="terp-check" string="Current" domain="[('state','in', ('draft','open'))]" help="Assets in draft and open states"/>
                    <filter icon="terp-dialog-close" string="Closed" domain="[('state','=', 'close')]" help="Assets in closed state"/>
                    <field name="category_id"/>
                    <field name="partner_id" filter_domain="[('partner_id','child_of',self)]"/>
                </search>
            </field>
        </record>
        <!--        Request form       -->
        <record model="ir.ui.view" id="view_custody_request_form">
            <field name="name">custody.request.form</field>
            <field name="model">asset.custody</field>
            <field name="arch" type="xml">
                <form string="Custody Request" version="7.0">
                    <header>
                        <button name="confirm" string="Confirm" states="draft" type="workflow" class="oe_highlight" />
                        <!--button name="approve" states="confirmed" string="Approve" confirm="Are you sure you want to approve this order?" class="oe_highlight"/-->
                        <button name="recived" states="confirmed" string="Recived" confirm="Are you sure you want to recieved this order?" class="oe_highlight"/>
                        <button name="cancel" string="Cancel" confirm="Are you sure you want to cancel this?" states="draft,confirmed,approved" type="workflow" class="oe_highlight" icon="gtk-cancel"/>
                        <button name="action_cancel_draft" states="canceled" string="Set to Draft" type="object" icon="gtk-convert"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,confirmed,approved,recived,canceled" />
                    </header>
                    <sheet>
                        <group col="4" colspan="4">
                            <field name="name" />
                            <field name="type" />
                            <field name="date" />
                            <field name="executing_agency" required='True' />
                            <field name="custody_type" required='True' attrs="{'readonly' : [('state','not in',('draft'))]}" />
                            <field name="department_to" attrs="{'readonly' : [('state','not in',('draft'))]  }" />
                            <field name="employee_id" attrs="{'readonly' : [('state','not in',('draft'))] }" />
                            <field name="stock" invisible="1"/>
                            <field name="temporary_custody" invisible="1"/>
                        </group>
                        <separator colspan="4"/>
                        <button name="create_line" type="object" string="create" attrs="{'invisible' : [('state','not in',('draft'))]}" />
                        <!--group col="4" colspan="4" string="in case of genral data "><field name="time_scale" required='True'/><field name="return_date" required='False' attrs="{'required' : [('time_scale','=','temporary')]}"/><field name="request_date" /></group-->
                        <notebook>
                            <page string="Asset Line">
                                <field name="asset_line" attrs="{'readonly' : [('state','not in',('confirmed','draft'))]}">
                                    <tree string="Asset " editable="buttom">
                                        <field name="executing_agency" />
                                        <field name="department_to" />
                                        <field name="office"  />
                                        <field name="custody_type" required='True' />
                                        <field name="employee_to" domain="[('department_id','=', department_to)]" on_change="on_change_employee(employee_to)" />
                                        <field name="asset_id" />
                                        <field name="product_id" required='True' domain="[('custody','=', True)]" />
                                        <field name="serial_no" />
                                        <field name="qty" />
                                        <field name="state_rm" required='True'/>
                                        <!--field name="model" readonly='True'/-->
                                        <!--field name="department_from" readonly="1"/-->
                                        <!--field name="employee_from" readonly="1"/-->
                                        <field name="referance" invisible="1"/>
                                        <field name="date" invisible="1" />
                                        <field name="date_done" invisible="1"/>
                                        <field name="location_from" invisible="1"/>
                                        <field name="location_to" domain="[('custody','=',False)]" readonly="0" invisible="1"/>
                                        <field name="time_scale" required='True'/>
                                        <field name="return_date" required='False' attrs="{'required' : [('time_scale','=','temporary')], 'readonly' : [('time_scale','=','constant')] }" />
  
 
                                    </tree>
                                </field>
                            </page>
                        </notebook >
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="view_custody_request_tree">
            <field name="name">custody.request.tree</field>
            <field name="model">asset.custody</field>
            <field name="arch" type="xml">
                <tree string="Custody Request">
                    <field name="executing_agency" />
                    <field name="department_to" />
                    <field name="employee_to" />
                    <field name="name"/>
  
                    <field name="custody_type" />
 
                    <field name="date" />
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record id="view_custody_request_search" model="ir.ui.view">
            <field name="name">custody.request.search</field>
            <field name="model">asset.custody</field>
            <field name="arch" type="xml">
                <search string="Custody Request">
                    <group string="Filters">
                        <filter name="personal" string="Personal" domain="[('custody_type','=','personal')]" separator="1" />
