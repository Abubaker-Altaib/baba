        <!--    Return form      -->
        <record model="ir.ui.view" id="view_custody_return_form">
            <field name="name">custody.return.form</field>
            <field name="model">asset.custody</field>
            <field name="arch" type="xml">
                <form string="Custody Return" version="7.0">
                    <header>
                        <button name="confirm" string="Confirm" states="draft" type="workflow" class="oe_highlight" />
                        <!--button name="approve" states="confirmed" string="Approve" confirm="Are you sure you want to approve this order?" class="oe_highlight"/-->
                        <button name="recived" states="confirmed" string="Recived" confirm="Are you sure you want to recieved this order?" class="oe_highlight"/>
                        <button name="cancel" string="Cancel" confirm="Are you sure you want to cancel this?" states="draft,confirmed,approved" type="workflow" class="oe_highlight" icon="gtk-cancel"/>
                        <button name="action_cancel_draft" states="canceled" string="Set to Draft" type="object" icon="gtk-convert"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,confirmed,approved,recived,canceled" />
                    </header>
                    <sheet>
                        <group col="4" colspan="4">
                            <field name="name"/>
                            <field name="type" />
                            <field name="date" />
                            <field name="custody_type" required ='1' attrs="{'readonly' : [('state','not in',('draft'))]}"/>
                            <field name="department_id" attrs="{'readonly' : [('state','not in',('draft'))]}" />
                            <field name="executing_agency" attrs="{'readonly' : [('state','not in',('draft'))]}" />
                        </group>
                        <group col="4" colspan="4">
                            <field name="employee_id" attrs="{'readonly' : [('state','not in',('draft'))]}" />
                            <field name="employee_id1" attrs="{'readonly' : [('state','not in',('draft'))]}" />
                            <field name="employee_id2" attrs="{'readonly' : [('state','not in',('draft'))]}" />
                        </group>
                        <button name="create_line" type="object" string="create" attrs="{'invisible' : [('state','not in',('draft'))]}" />
                        <notebook >
                            <page string="Asset Line">
                                <field name="asset_line" attrs="{'readonly' : [('state','not in',('approved','draft'))]}">
                                    <tree string="Asset Lines" editable="buttom">
                                        <field name="executing_agency" required='True' on_change="on_change_data(department_to,office,product_id,executing_agency,custody_type,employee_to)"/>
                                        <field name="custody_type" required='True' on_change="on_change_data(department_to,office,product_id,executing_agency,custody_type,employee_to)" />
                                        <field name="department_to" readonly='0' on_change="on_change_data(department_to,office,product_id,executing_agency,custody_type,employee_to)" attrs="{'required' : [('custody_type','=','management')], }" />
                                        <field name="office" readonly='0' on_change="on_change_data(department_to,office,product_id,executing_agency,custody_type,employee_to)" attrs="{'required' : [('custody_type','=','management')],'readonly' : [('custody_type','=','personal')]}" />
                                        <field name="employee_to" readonly='0' on_change="on_change_data(department_to,office,product_id,executing_agency,custody_type,employee_to)" attrs="{'required' : [('custody_type','=','personal')],'readonly' : [('custody_type','=','management')]}" />
                                        <field name="asset_id" domain="[('asset_type','=','custody')]" context="{'form_view_ref':'account_asset_custody.view_account_asset_asset_form2'}" required="1"/>
                                        <field name="product_id" required='True' on_change="on_change_data(department_to,office,product_id,executing_agency,custody_type,employee_to)"/>
                                        <field name="time_scale" />
                                        <field name="return_date" invisible="1" />
                                        <field name="request_date" invisible="1" />
                                        <field name="referance" invisible="1" />
                                        <field name="date" invisible="1" />
                                        <field name="date_done" invisible="1"/>
                                        <field name="location_from" invisible="1"/>
                                        <field name="location_to" invisible="1" />
                                        <field name="serial_no" invisible="1" />
                                        <field name="return_type" attrs="{'required' : [('state','not in',('draft'))]}" />
                                        <field name="state_rm" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                            <field name="note" colspan="4" nolabel="1" required="False" />
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
