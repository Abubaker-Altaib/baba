        <!--  REQUEST ACTION  -->
        <record model="ir.actions.act_window" id="action_custody_request">
            <field name="name">Custody Request</field>
            <field name="res_model">asset.custody</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{"type":'request'}</field>
