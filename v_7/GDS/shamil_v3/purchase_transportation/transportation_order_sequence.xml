            <field name="name">Transportation quotes</field>
            <field name="code">transportation.quotes</field>
            <field name="prefix">%(year)s/TQ-</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_type_transportation_driver" model="ir.sequence.type">
            <field name="name">Transportation Driver</field>
            <field name="code">transportation.driver</field>
        </record>
        <record id="seq_transportation_driver" model="ir.sequence">
            <field name="name">Transportation Driver</field>
            <field name="code">transportation.driver</field>
            <field name="prefix">%(year)s/TD-</field>
